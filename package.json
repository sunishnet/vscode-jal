{
    "name": "jal",
    "displayName": "JAL",
    "description": "Just Another Language for PIC microcontrollers",
    "version": "2021.0.2",
    "author": "Sunish Issac",
    "publisher": "suniship",
    "engines": {
        "vscode": "^1.54.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "jal",
            "aliases": ["Just Another Language (jal)", "jal"],
            "extensions": [".jal"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "jal",
            "scopeName": "source.jal",
            "path": "./syntaxes/jal.tmLanguage.json"
        }],
        "snippets": [{
            "language": "jal",
            "path": "./snippets/jalsnippets.json"
        }],
        "configuration": {
            "title": "jal",
            "properties": {
                "jal.paths.exePath": {
                    "type": "string",
                    "default": "D://jallib1.5//compiler//jalv2_64.exe",
                    "description": "Exe path of JALV2.exe."
                },
                "jal.paths.LibPath": {
                    "type": "string",
                    "default": "D://jallib1.5//lib",
                    "description": "Library paths of jal.(More than one path can be appended with semicolon(;)"
                },
                "jal.paths.documentPath": {
                    "type": "string",
                    "default": "D://jallib1.5//doc",
                    "description": "Document path of jal."
                },
                "jal.paths.programmerPath": {
                    "type": "string",
                    "default": "D://jallib1.5//pickit.bat",
                    "description": "Programmer Path"
                }
            }
        }

    }
}
    "tokenColors": [{
            "settings": {
                "foreground": "#333333ff",
                "background": "#f5f5f5ff"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "comment.control"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#008080"
            }
        },
        {
            "name": "Invalid - Illegal",
            "scope": "invalid.illegal",
            "settings": {
                "background": "#96000014",
                "foreground": "#660000"
            }
        },
        {
            "name": "Support",
            "scope": "support.type",
            "settings": {
                "foreground": "#61AFEF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Keywords:Control",
            "scope": [
                "keyword.control",
                "storage"
            ],
            "settings": {
                "foreground": "#E5C07B",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Keywords:Operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#C678DD",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Keywords:Other",
            "scope": [
                "keyword.control.other",
                "storage"
            ],
            "settings": {
                "foreground": "#E5C07B",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type"
            ],
            "settings": {
                "foreground": "#D19A66",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#E06C75",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#98C379"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#98C379"
            }
        },
        {
            "name": "Numbers, Characters",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#98C379"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#CE9178",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strings: Escape Sequences",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#ff4dff",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Embedded Source",
            "scope": [
                "string source",
                "text source"
            ],
            "settings": {
                "background": "#EAEBE6"
            }
        }
    ]
}